{
  "settings": {
    "react": {
      "version": "17.0.2"
    }
  },
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "jest": true,
    "jquery": true
  },
  "extends": [
    "eslint:recommended",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [
  ],
  "rules": {
    "no-console": "off",
    "no-unused-vars": [
      "error",
      {
        "ignoreRestSiblings": true
      }
    ]
  }
}